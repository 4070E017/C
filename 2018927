6錯誤
#include <stdio.h>
#include <stdlib.h>
int main(void)
{
   printf('Hello 偉大的恩師!\n';   	/* 印出Hello 偉大的恩師! 字串 */
   printf("Hello Taiwan!\n");   /* 印出Hello Taiwan! 字串 */   
   
   system("pause");
   return 0 ;
   printf("Hello World!\n");
}
7錯誤
#include <stdio.h>
#include <stdlib.h>
int main(void)
{
   printf('Hello 偉大的恩師!\n');   	/* 印出Hello 偉大的恩師! 字串 */
   printf("Hello Taiwan!\n";   /* 印出Hello Taiwan! 字串 */   
   
   system("pause");
   return 0 ;
   printf("Hello World!\n");
}
8錯誤
#include <stdio.h>
#include <stdlib.h>
int main(void)
{
   printf('Hello 偉大的恩師!\n');   	/* 印出Hello 偉大的恩師! 字串 */
   printf("Hello Taiwan!\n"; )  /* 印出Hello Taiwan! 字串 */   
   
   system("pause");
   returnR 0 ;
   printf("Hello World!\n");
}
